{
  "name": "zcli-action",
  "version": "1.0.0",
  "description": "Github Action to deploy Zendesk Apps using ZCLI.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "prepare": "ncc build index.js -o dist --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run lint && npm run prepare && npm run test",
    "build:prod": "tsc && vite build",
    "zcli": "zcli apps:server",
    "deploy": "yarn build && zcli apps:update dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eteg/zcli-action.git"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "JavaScript"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eteg/zcli-action/issues"
  },
  "homepage": "https://github.com/eteg/zcli-action/blob/master/README.md",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.0.3",
    "@zendesk/zcli": "^1.0.0-beta.16",
    "shelljs": "^0.8.5",
    "dotenv": "^16.0.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.23.0",
    "jest": "^28.1.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "@zendesk/zcli": "^1.0.0-beta.16",
    "rollup-plugin-copy": "^3.4.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.9",
    "vite-tsconfig-paths": "^3.5.0"
  }
}
